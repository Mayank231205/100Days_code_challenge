/* 100 Days Code; Day 41: Code 2 */
#include<stdio.h>
int main()
{
    printf("Mayank Singh\nSAP ID: 590026159\nCourse- BSc CS\nBatch-1");
    printf("\n-----------------\n");

    int n;
    printf("Enter number: ");
    scanf("%d", &n);

    printf("Factors of %d are: ", n);
    for(int i = 1; i <= n; i++)
        if(n % i == 0)
            printf("%d ", i);
    return 0;
}
