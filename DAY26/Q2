int main()
{
        printf("Mayank Singh\nSAP ID: 590026159\nCourse- BSc CS\nBatch-1"); 
    printf("\n-----------------\n");

    char s1[100], s2[100];
    printf("Enter first string: ");
    gets(s1);
    printf("Enter second string: ");
    gets(s2);

    strcat(s1, s2);
    printf("Concatenated string: %s", s1);
    return 0;
}
