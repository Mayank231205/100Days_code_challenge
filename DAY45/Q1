/* 100 Days Code; Day 45: Code 1 */
#include<stdio.h>
int main()
{
    printf("Mayank Singh\nSAP ID: 590026159\nCourse- BSc CS\nBatch-1");
    printf("\n-----------------\n");

    int a, b, temp;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    temp = a;
    a = b;
    b = temp;

    printf("After swapping: a = %d, b = %d", a, b);
    return 0;
}
