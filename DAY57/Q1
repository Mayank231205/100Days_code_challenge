/* 100 Days Code; Day 57: Code 1 */
#include<stdio.h>
int main()
{
    printf("Mayank Singh\nSAP ID: 590026159\nCourse- BSc CS\nBatch-1");
    printf("\n-----------------\n");

    int n, fact = 1;
    printf("Enter number: ");
    scanf("%d", &n);

    for(int i = 1; i <= n; i++)
        fact *= i;

    printf("Factorial = %d", fact);
    return 0;
}
