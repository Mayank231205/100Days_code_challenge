/* 100 Days Code; Day 63: Code 1 */
#include<stdio.h>
int main()
{
    printf("Mayank Singh\nSAP ID: 590026159\nCourse- BSc CS\nBatch-1");
    printf("\n-----------------\n");

    int n;
    printf("Enter size: ");
    scanf("%d", &n);

    for(int i = 1; i <= n; i++)
    {
        for(int j = 1; j <= n; j++)
            printf("* ");
        printf("\n");
    }
    return 0;
}
