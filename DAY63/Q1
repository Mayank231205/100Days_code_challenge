/* 100 Days Code; Day 62: Code 1 */
#include<stdio.h>
int main()
{
    printf("Mayank Singh\nSAP ID: 590026159\nCourse- BSc CS\nBatch-1");
    printf("\n-----------------\n");

    int n, a[100];
    printf("Enter size: ");
    scanf("%d", &n);

    printf("Enter elements: ");
    for(int i = 0; i < n; i++)
        scanf("%d", &a[i]);

    printf("Reverse: ");
    for(int i = n-1; i >= 0; i--)
        printf("%d ", a[i]);

    return 0;
}
